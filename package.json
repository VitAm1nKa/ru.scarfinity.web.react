{
  "name": "ru.trisquare.scarfinity.web.react",
  "version": "0.1.0",
  "description": "Scarfinity Web.React Project",
  "main": "webpack.config.js",
  "scripts": {
    "vendor": "webpack --config webpack.config.vendor.js --env.prod",
    "build-dev": "webpack --config webpack.config.js",
    "build-prod": "webpack --config webpack.config.js --env.prod",
    "run-dev": "SET NODE_ENV=development&& webpack --color",
    "run-prod": "SET NODE_ENV=production&& webpack --color",
    "profile-dev": "SET NODE_ENV=development&& webpack --profile --json > stats.json",
    "profile-prod": "SET NODE_ENV=production&& webpack --profile --json > stats.json",
    "hot": "SET NODE_ENV=development&& webpack-dev-server --hot --colors",
    "cold": "SET NODE_ENV=development&& webpack-dev-server --colors",
    "watch-dev": "SET NODE_ENV=development&& webpack --watch --color",
    "watch-prod": "SET NODE_ENV=production&& webpack --watch --color",
    "devserver": "webpack-dev-server --config webpack.config.dev-server.js --env.dev --debug --hot --devtool eval-source-map --output-pathinfo --watch --colors --inline --content-base wwwroot --port 8095 --host 192.168.1.198"
  },
  "author": "Mikhail ru.trisquare.vitam1nka Silantyev",
  "license": "ISC",
